{
  "name": "baxi-ui",
  "version": "0.0.1",
  "description": "baxi UI",
  "main": "./lib/baxi-ui.common.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --config ./build/webpack.dev.config.js",
    "docs": "webpack-dev-server --inline --config ./build/webpack.docs.config.js",
    "prod": "webpack --config ./build/webpack.prod.config.js && npm run build:style && npm run build:font && npm run build:component",
    "build": "npm run build:docs && npm run build:example && webpack --config ./build/webpack.prod.config.js && npm run build:style && npm run build:font && npm run build:component",
    "build:style": "gulp --gulpfile build/bin/build-styles.js",
    "build:icon": "gulp --gulpfile build/bin/build-icons.js",
    "build:font": "gulp --gulpfile build/bin/build-fonts.js",
    "build:docs": "node build/docs/build.js",
    "build:example": "node build/examples/build.js",
    "build:component": "webpack --config ./build/webpack.component.config.js",
    "lint": "eslint --fix --ext .js,.vue src",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "codecov": "codecov",
    "test": "npm run lint && npm run unit && npm run codecov"
  },
  "author": "baxi",
  "keywords": [
    "components"
  ],
  "license": "MIT",
